{
  "Logging": {
    "Console": {
      "IncludeScopes": true,
      "LogToStandardErrorThreshold": "Information"
    },
    "LogLevel": {
      "Default": "Information",
      "GitStractor": "Debug",
      "GitStractor.Acquire": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "GitStractorHost": {
    "AllowEarlyStop": true
  },
  "Acquire": {
    "OverwriteIfExists": false
  }
}
